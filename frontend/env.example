# Konfiguracja frontendu React
# Skopiuj ten plik jako .env i ustaw swoje wartości
# Zmienne środowiskowe dla React muszą zaczynać się od REACT_APP_

# PayPal - Frontend (tylko Client ID, secret jest w backendzie)
REACT_APP_PAYPAL_CLIENT_ID=twoj_paypal_client_id
REACT_APP_PAYPAL_ENVIRONMENT=live

# Backend URL
REACT_APP_API_URL=https://twojadomena.pl

# Frontend URL
REACT_APP_FRONTEND_URL=https://twojadomena.pl
